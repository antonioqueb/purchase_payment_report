<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="action_purchase_payment_report" model="ir.actions.report">
        <field name="name">Reporte de Pagos a Proveedor</field>
        <field name="model">purchase.payment.report.wizard</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">purchase_payment_report.report_purchase_payment</field>
        <field name="report_file">purchase_payment_report.report_purchase_payment</field>
        <field name="print_report_name">'Pagos_Proveedor_' + object.report_title.replace(' ', '_')</field>
        <field name="binding_model_id" eval="False"/>
        <field name="paperformat_id" ref="base.paperformat_euro"/>
    </record>
</odoo>
